<?xml version="1.0" encoding="UTF-8"?>
<web-fragment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xmlns="http://java.sun.com/xml/ns/javaee"
			  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd"
			  id="Trace_SERVLET_FILTER" version="3.0">

	<filter>
		<filter-name>TraceFilter</filter-name>
		<filter-class>org.aoju.bus.tracer.binding.servlet.TraceFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>TraceFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<listener>
		<listener-class>org.aoju.bus.tracer.binding.servlet.TraceServletRequestListener</listener-class>
	</listener>
	<listener>
		<listener-class>org.aoju.bus.tracer.binding.servlet.TraceSessionListener</listener-class>
	</listener>
</web-fragment>
